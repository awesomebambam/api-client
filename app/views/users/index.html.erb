<h1>List of users</h1>

<table class='table table-bordered'>
  <tr>
    <th>user name</th>
    <th>first name</th>
    <th>last name</th>
  </tr>

  <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
      </tr>
  <% end %>
</table>
